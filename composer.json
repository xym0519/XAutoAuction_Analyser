{
    "name": "tourmaline/project",
    "version": "2.0.0",
    "description": "360CBS",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "tourmaline"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "laravel/lumen-framework": "^8.3.1",
        "tourmaline/infrastructure": "^2.0",
        "tourmaline/word": "^2.0",
        "tourmaline/utils-iod": "^2.0",
        "tourmaline/request-log": "^2.0",
        "tourmaline/db-log": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "tourmaline/cbsunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.project.360cbs.com:8090"
        }
    ]
}
